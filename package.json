{
  "name": "music-theory-toolkit",
  "version": "0.9.0",
  "description": "Note/chord/set parsing, transposition, and enumeration tools",
  "main": "./lib/index.js",
  "scripts": {
    "preversion": "npm test",
    "build": "rimraf lib && npm run compile",
    "compile": "tsc",
    "test": "nyc mocha -r ts-node/register test/**/*.spec.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "docs:gen": "npm run docs:clear && typedoc src --mode file --readme none --hideGenerator",
    "docs:clear": "rimraf docs"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "files": [
    "lib/"
  ],
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "music",
    "chord",
    "note",
    "chords",
    "notes",
    "transposition",
    "enumeration",
    "composition"
  ],
  "author": "Matt Cook <matt@mattcook.xyz> (https://mattcook.xyz)",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-security": "^1.16.0",
    "typedoc": "^0.15.1",
    "typedoc-plugin-markdown": "^2.2.11"
  },
  "dependencies": {
    "@types/lodash": "^4.14.141",
    "lodash": "^4.17.11",
    "typescript": "^3.6.3"
  }
}
