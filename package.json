{
  "name": "music-theory-toolkit",
  "version": "0.6.0",
  "description": "Note/chord/set parsing, transposition, and enumeration tools",
  "main": "./lib/index.js",
  "scripts": {
    "preversion": "npm test",
    "build": "npm run compile && npm run copy-types",
    "compile": "babel src -d lib",
    "copy-types": "cp src/index.d.ts lib/index.d.ts",
    "setup-docs": "echo '[Click here to return to the readme.](../readme.md)\n' > docs/models.md",
    "create-docs": "npm run setup-docs && jsdoc2md src/models/*.js >> docs/models.md",
    "test": "nyc mocha -r @babel/register test/*.spec.js",
    "lint": "eslint src/**/*.js"
  },
  "files": [
    "lib/"
  ],
  "pre-commit": [
    "lint",
    "test"
  ],
  "keywords": [
    "music",
    "chord",
    "note",
    "chords",
    "notes",
    "transposition",
    "enumeration",
    "composition"
  ],
  "author": "Matt Cook <matt@mattcook.xyz> (https://mattcook.xyz)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.15.1",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
