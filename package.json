{
  "name": "music-theory-sdk",
  "version": "0.1.2",
  "description": "Note/chord/set parsing, transposition, and enumeration tools",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "babel src -d lib",
    "preversion": "npm test",
    "build": "babel src -d lib",
    "test": "nyc mocha -r @babel/register test/*.spec.js",
